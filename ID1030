//TRIPLE FAT LADIES
#include <iostream>
#include <bits/stdc++.h>
#include <stdio.h>
#include <stdlib.h>
#include <string>
using namespace std;

int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        long long k;
        scanf("%lld",&k);
        long long ans=192;
        for(int i=1;i<k;i++)
        {
            ans+=250;
        }
        printf("%lld\n",ans);
    }
}
